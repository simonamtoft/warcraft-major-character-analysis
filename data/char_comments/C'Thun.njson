sername": "Ahuramazda", "markupDisabledEnvironments": {"ptr": true, "beta": false}, "downvotedByUser": false, "reportedByUser": false, "votedByUser": false}, {"body": "Makes me wonder how many  WLK 25 man geared people would it take to powerhouse him down with sheer force, with minimal / none resistances.", "creationdate": "2008-12-24T18:34:17-06:00", "id": 67459, "rating": 16, "commentid": 2298, "dataTree": 1, "roles": 0, "username": "289054", "markupDisabledEnvironments": {"ptr": true, "beta": false}, "downvotedByUser": false, "reportedByUser": false, "votedByUser": false}], "dataTree": 1, "markupDisabledEnvironments": {"ptr": true, "beta": false}}
